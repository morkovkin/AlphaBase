<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="316.0" prefWidth="719.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.egor.database.alphabase.AddCompanyController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <VBox prefHeight="62.0" prefWidth="793.0">
      <children>
         <Label text="Company info" />
         <HBox prefHeight="28.0" prefWidth="793.0">
            <children>
               <TextField fx:id="tfInn" promptText="inn" />
               <TextField fx:id="tfIfns" prefHeight="26.0" prefWidth="218.0" promptText="ifns" />
               <TextField fx:id="tfCompanyName" prefHeight="26.0" prefWidth="277.0" promptText="name" />
            </children>
         </HBox>
      </children>
   </VBox>
   <VBox prefHeight="100.0" prefWidth="376.0">
      <children>
         <Label text="Owner info" />
         <TextField fx:id="ownerName" promptText="Contact Name">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin></TextField>
         <TextField fx:id="ownerPhone" promptText="+7(XXX)XXX-XX-XX">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin></TextField>
         <TextField fx:id="ownerEmail" prefHeight="26.0" prefWidth="543.0" promptText="Contact email">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin></TextField>
      </children>
      <VBox.margin>
         <Insets bottom="10.0" />
      </VBox.margin>
   </VBox>
   <Button mnemonicParsing="false" onAction="#onSafeClick" prefHeight="26.0" prefWidth="265.0" text="save" />
</VBox>
